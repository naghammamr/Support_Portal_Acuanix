    <test name="UAR Test" preserve-order="true">
        <classes>
            <class name="tests.loginTest"/>
            <methods>
                <include name="superAdminCanLoginToThePortal"/>
                <exclude name = "CustomerLoginFromExcel"/>
            </methods>
            <class name="tests.T_PriorityTest"/>
            <methods>
                <include name="userCanNavigateToTicketPriority"/>
                <include name="userCanAddNewPriority"/>
            </methods>

        </classes>
    </test>
</suite>